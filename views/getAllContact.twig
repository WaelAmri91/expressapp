<body>
        <h1 style="text-align: center">List of Contact</h1>
        <form class="d-flex" method="post" action="/contact/search">
    <input class="form-control me-2" name="Search" type="search" placeholder="Search" id="Search" >
    <button class="btn btn-outline-success" type="submit">Search</button>
</form>

<table border="1">
                <tr>
                        <td>nom</td>
                        <td>phone</td>
                        <td>action</td>
                </tr>


        {% for item in data %}
                <tr>
                        <td>{{item.FullName}}</td>
                        <td>{{ item.Phone }}</td>
                        <td><a href="/contact/delete/{{item._id}}">Delete</a></td>
                        <td><a href="/contact/update/{{item._id}}">Update</a></td>
                </tr>

        {% endfor %}
</table>
</body>

